div(class="col s4",id="right-container")
    ul(class="collection with-header")
        li(class="collection-header")
            h4
                | &nbsp;Links
        li(class="collection-header")
         h6 
                | &nbsp;Title
         h6     | &nbsp;Description
         h6     | &nbsp;Path      
        each item in right
            li(class="collection-item hoverable")
                div=item.title
                    a(target="_blank",href=item.link,class="secondary-content")
                        i(class="material-icons")=item.icon
        li(class="collection-item") &nbsp;
